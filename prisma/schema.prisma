// prisma/schema.prisma

datasource db {
 provider = "postgresql"
 url      = env("DATABASE_URL")
}

generator client {
 provider = "prisma-client-js"
}

// prisma/schema.prisma

model User {
 id        String   @id @default(cuid())
 email     String   @unique
 name      String?
 image     String?
 createdAt DateTime @default(now())
 updatedAt DateTime @updatedAt
}

